set_instance_assignment -name WEAK_PULL_UP_RESISTOR ON -to B
set_instance_assignment -name IO_STANDARD "3.3V SCHMITT TRIGGER INPUT" -to B
#=======================================================
# Displays:
# Segmentos do disp2 - descomentar e atribuir se utilizado:
set_location_assignment PIN_30 -to OR_OUT   # a - A or B
set_location_assignment PIN_34 -to B2     	# b
set_instance_assignment -name RESERVE_PIN AS_OUTPUT_DRIVING_GROUND -to B2
set_location_assignment PIN_36 -to C2       # c
set_instance_assignment -name RESERVE_PIN AS_OUTPUT_DRIVING_GROUND -to C2
set_location_assignment PIN_38 -to XNOR_OUT # d - A xnor B
set_location_assignment PIN_40 -to E2       # e
set_instance_assignment -name RESERVE_PIN AS_OUTPUT_DRIVING_GROUND -to E2
set_location_assignment PIN_42 -to F2       # f
set_instance_assignment -name RESERVE_PIN AS_OUTPUT_DRIVING_GROUND -to F2
set_location_assignment PIN_44 -to NOR_OUT  # g - A nor B
set_location_assignment PIN_48 -to NOT_B    # dp - Not B
#=========================================================
# Segmentos do disp1:
set_location_assignment PIN_33 -to AND_OUT  # a - A and B
set_location_assignment PIN_35 -to B1       # b
set_instance_assignment -name RESERVE_PIN AS_OUTPUT_DRIVING_GROUND -to B1
set_location_assignment PIN_37 -to C1       # c
set_instance_assignment -name RESERVE_PIN AS_OUTPUT_DRIVING_GROUND -to C1
set_location_assignment PIN_39 -to XOR_OUT  # d - A xor B
set_location_assignment PIN_41 -to E1       # e
set_instance_assignment -name RESERVE_PIN AS_OUTPUT_DRIVING_GROUND -to E1
set_location_assignment PIN_43 -to F1       # f
set_instance_assignment -name RESERVE_PIN AS_OUTPUT_DRIVING_GROUND -to F1
set_location_assignment PIN_47 -to NAND_OUT # g - A nand B
set_location_assignment PIN_49 -to NOT_A    # dp - Not A
